{
  "name": "claude-code-templates",
  "version": "1.0.0",
  "description": "Claude Code Templates - Open Source Documentation and Templates for Claude Code Development",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "dev": "npm run start",
    "build:prod": "npm run build && npm run serve",
    "deploy:gh-pages": "npm run build && gh-pages -d build",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@docusaurus/core": "3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@mdx-js/react": "^3.0.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.58.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "@eslint/js": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "claude-code",
    "ai-assisted-development",
    "subagents",
    "hooks",
    "workflow-automation",
    "documentation",
    "templates",
    "opensource"
  ],
  "author": "Claude Code Templates Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davila7/claude-code-templates.git"
  },
  "homepage": "https://davila7.github.io/claude-code-templates/"
}
